{
  "name": "nuxt-ssr",
  "version": "1.0.0",
  "description": "My doozie Nuxt.js project",
  "author": "zzp-dog",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
